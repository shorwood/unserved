{
  "name": "@unserved/server",
  "type": "module",
  "version": "0.3.1",
  "license": "MIT",
  "sideEffects": false,
  "author": "Stanley Horwood <stanley@hsjm.io>",
  "bugs": "https://github.com/shorwood/unserved/issues",
  "homepage": "https://github.com/shorwood/unserved#readme",
  "repository": {
    "directory": "packages/server",
    "type": "git",
    "url": "git@github.com:shorwood/unserved.git"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./BaseEntity": {
      "require": "./dist/BaseEntity.cjs",
      "types": "./dist/BaseEntity.d.ts",
      "import": "./dist/BaseEntity.js"
    },
    "./__fixtures__": {
      "require": "./dist/__fixtures__.cjs",
      "types": "./dist/__fixtures__.d.ts",
      "import": "./dist/__fixtures__.js"
    },
    "./createApplication": {
      "require": "./dist/createApplication.cjs",
      "types": "./dist/createApplication.d.ts",
      "import": "./dist/createApplication.js"
    },
    "./createError": {
      "require": "./dist/createError.cjs",
      "types": "./dist/createError.d.ts",
      "import": "./dist/createError.js"
    },
    "./createHttpRoute": {
      "require": "./dist/createHttpRoute.cjs",
      "types": "./dist/createHttpRoute.d.ts",
      "import": "./dist/createHttpRoute.js"
    },
    "./createModule": {
      "require": "./dist/createModule.cjs",
      "types": "./dist/createModule.d.ts",
      "import": "./dist/createModule.js"
    },
    "./createWebSocketRoute": {
      "require": "./dist/createWebSocketRoute.cjs",
      "types": "./dist/createWebSocketRoute.d.ts",
      "import": "./dist/createWebSocketRoute.js"
    },
    "./types": {
      "require": "./dist/types.cjs",
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js"
    },
    "./utils": {
      "require": "./dist/utils.cjs",
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.js"
    },
    ".": {
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE.md"
  ],
  "peerDependencies": {
    "crossws": ">=0.3.1",
    "h3": ">=1.13.0",
    "typeorm": ">=0.3.20"
  },
  "dependencies": {
    "@unshared/decorators": "0.4.1",
    "@unshared/functions": "0.4.1",
    "@unshared/process": "0.4.1",
    "@unshared/string": "0.4.1",
    "@unshared/types": "0.4.1",
    "@unshared/validation": "0.4.1",
    "reflect-metadata": "0.2.2",
    "ws": "8.18.0"
  },
  "devDependencies": {
    "@types/ws": "8.5.14",
    "crossws": "0.3.3",
    "h3": "1.14.0",
    "typeorm": "0.3.20"
  }
}
